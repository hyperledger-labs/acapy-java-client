<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
    xmlns="https://github.com/spotbugs/filter/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
    </Match>

    <!-- Ignore warnings for Lombok builder classes working with @Singular on maps and lists -->
    <Match>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
        <And>
            <Or>
                <Method  name="~clear.+" />
                <Method  name="build" />
            </Or>
            <Class name="~.+Builder.*"/>
        </And>
    </Match>

    <Match>
       <Class name="org.hyperledger.aries.webhook.WebSocketListener" />
       <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS" />
    </Match>
    
    <!-- Exclude generated code -->
    <Match>
        <Package name="org.hyperledger.acy_py.generated.model" />
    </Match>

    <Match>
       <Class name="org.hyperledger.aries.BaseClient" />
       <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE,DLS_DEAD_LOCAL_STORE" />
    </Match>

    <Match>
       <Class name="org.hyperledger.aries.AriesClient" />
       <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE" />
    </Match>

    <Match>
       <Class name="org.hyperledger.aries.AriesClient$AriesClientBuilder" />
       <Bug pattern="URF_UNREAD_FIELD" />
    </Match>

    <Match>
       <Class name="~.*org.hyperledger.aries.api.jsonld.Verifiable.*" />
       <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD" />
    </Match>

    <Match>
       <Class name="org.hyperledger.aries.api.present_proof.PresentationRequest" />
       <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD" />
    </Match>

    <Match>
        <Class name="org.hyperledger.aries.api.present_proof.PresentProofRequest$ProofRequest" />
        <Bug pattern="SE_BAD_FIELD,SE_NO_SERIALVERSIONID" />
    </Match>

    <Match>
        <Bug pattern="CRLF_INJECTION_LOGS" />
    </Match>

</FindBugsFilter>